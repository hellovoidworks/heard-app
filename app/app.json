{
  "expo": {
    "name": "Heard",
    "slug": "heard-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "jsEngine": "hermes",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#161616"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "co.voidworks.heardapp",
      "associatedDomains": [
        "applinks:lrdylsehrfkkrjzicczz.supabase.co"
      ],
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "heardapp",
              "co.voidworks.heardapp"
            ]
          }
        ],
        "UIBackgroundModes": ["remote-notification"],
        "CFBundleDisplayName": "Heard",
        "ITSAppUsesNonExemptEncryption": false
      },
      "useFrameworks": "static",
      "frameworks": [
        "AdSupport.framework",
        "AdServices.framework",
        "StoreKit.framework"
      ],
      "buildSettings": {
        "TARGETED_DEVICE_FAMILY": "1",
        "SUPPORTS_MACCATALYST": false,
        "SUPPORTS_MAC_DESIGNED_FOR_IPHONE_IPAD": false,
        "SUPPORTS_XR_DESIGNED_FOR_IPHONE_IPAD": false,
        "DEVELOPMENT_TEAM": "DKQ9N3J9SS"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "co.voidworks.heardapp",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "lrdylsehrfkkrjzicczz.supabase.co",
              "pathPrefix": "/auth/v1/callback"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "heardapp",
    "plugins": [
      "expo-apple-authentication",
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#161616"
        }
      ]
    ],
    "notification": {
      "icon": "./assets/notification-icon.png",
      "color": "#161616",
      "iosDisplayInForeground": true,
      "androidMode": "default",
      "androidCollapsedTitle": "${title}"
    },
    "extra": {
      "eas": {
        "projectId": "a3a69752-4412-4edf-ab67-fde51fdcfe0f"
      }
    },
    "build": {
      "production": {
        "ios": {
          "credentialsSource": "local",
          "appleTeamId": "DKQ9N3J9SS"
        }
      }
    }
  }
}